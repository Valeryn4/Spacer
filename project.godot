config_version=5

[application]

config/name="Spacer"
config/features=PackedStringArray("4.2", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

Global="*res://spacer/globals/global.gd"
GameDB="*res://spacer/globals/game_db.gd"
FractionManager="*res://spacer/content/fraction/fraction_manager.gd"
ShipBuilder="*res://spacer/content/ship_builder/ship_builder.gd"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"

[editor_plugins]

enabled=PackedStringArray("res://addons/codeandweb.texturepacker/plugin.cfg")

